package ua.nure.zabara.Practice5;

public class Spam extends Thread {

    long [] mSec;
    String [] strings;

    Spam(long[] mSec, String[] strings){
        this.mSec=mSec;
        this.strings=strings;
    }

    public void run(){
        for (int i = 0; i<mSec.length; i++) {
            try {
                Thread.sleep(mSec[i]);
                System.out.println(strings[i]);
            } catch (InterruptedException e) {
                break;
            }

        }

    }

}
