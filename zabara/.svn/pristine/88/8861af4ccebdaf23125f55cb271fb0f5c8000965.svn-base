public class Demo {

    public static void main(String[] args) {
        MyListImpl con=new MyListImpl();
        con.add("0");
        con.add("1");
        con.add("2");
        con.add("3");
        con.add("4");
        con.add(null);
        con.add("5");
        con.add("6");
        con.remove(null);



        ListIterator lt=con.listIterator();
        while (lt.hasNext()){
            System.out.println(lt.next());
        }
        while (lt.hasPrevious()){
            System.out.println(lt.previous());
        }
        System.out.println("/////////////////////////////////////////////");
        System.out.println(con.toString());
    }
}
