package ua.nure.zabara.Practice6.part4;

public class Part4 {
	
	public static void main(String[] args) {

        Vertex e = new Vertex(6);
        Graph gr=new Graph(5);
        gr.addFirst(e);
        Vertex e1=new Vertex(1);
        gr.add(e,e1);
        gr.add(e,new Vertex(2));
        gr.add(e,new Vertex(3));

        gr.add(e1,new Vertex(2));


        for(Vertex v:gr.vertexList){
            v.printConnections();
        }
	}

}
