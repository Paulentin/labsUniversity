public class Part4 {
    public static void main(String[] args) {
        int sum=0;
        for (int i = 0; i < args.length ; i++) {
            int number=Integer.parseInt(args[i]);

            while(number>0){
                sum+=number%10;
                number=number/10;

            }

        }

        System.out.println(sum);
    }
}
